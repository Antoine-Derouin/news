insert:
INSERT
INTO Log(date, level, tag, message, stackTrace)
VALUES (?, ?, ?, ?, ?);

selectAll:
SELECT *
FROM Log
ORDER BY date DESC;

selectById:
SELECT *
FROM Log
WHERE id = ?;

selectLastInsertRowId:
SELECT last_insert_rowid();

selectCount:
SELECT COUNT(*)
FROM Log;

deleteAll:
DELETE
FROM Log;

deleteWhereDateLessThan:
DELETE
FROM Log
WHERE date < :date;
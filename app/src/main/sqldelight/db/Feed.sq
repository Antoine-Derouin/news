CREATE TABLE Feed (
  id TEXT PRIMARY KEY NOT NULL,
  title TEXT NOT NULL,
  selfLink TEXT NOT NULL,
  alternateLink TEXT NOT NULL,
  openEntriesInBrowser INTEGER AS Boolean NOT NULL DEFAULT 0,
  blockedWords TEXT NOT NULL DEFAULT ''
);

insertOrReplace:
INSERT OR REPLACE
INTO Feed
VALUES ?;

selectAll:
SELECT *
FROM Feed
ORDER BY title;

selectById:
SELECT *
FROM Feed
WHERE id = ?;

deleteAll:
DELETE
FROM Feed;

deleteById:
DELETE
FROM Feed
WHERE id = ?;
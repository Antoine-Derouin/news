CREATE TABLE EntryEnclosure (
  entryId TEXT PRIMARY KEY NOT NULL,
  downloadPercent INTEGER,
  cacheUri TEXT NOT NULL DEFAULT ''
);

insertOrReplace:
INSERT OR REPLACE
INTO EntryEnclosure
VALUES ?;

selectAll:
SELECT *
FROM EntryEnclosure;

selectByEntryId:
SELECT *
FROM EntryEnclosure
WHERE entryId = ?;

deleteAll:
DELETE
FROM EntryEnclosure;

deleteByEntryId:
DELETE
FROM EntryEnclosure
WHERE entryId = ?;

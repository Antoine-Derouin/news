insertOrReplace:
INSERT OR REPLACE
INTO EntryEnclosure(entryId, downloadPercent, cacheUri)
VALUES ?;

selectAll:
SELECT *
FROM EntryEnclosure;

selectByEntryId:
SELECT *
FROM EntryEnclosure
WHERE entryId = ?;

setDowloadPercent:
UPDATE EntryEnclosure
SET downloadPercent = ?
WHERE entryId = ?;

setCacheUri:
UPDATE EntryEnclosure
SET cacheUri = ?
WHERE entryId = ?;

deleteAll:
DELETE
FROM EntryEnclosure;

deleteByEntryId:
DELETE
FROM EntryEnclosure
WHERE entryId = ?;
CREATE TABLE PodcastDownload (
  entryId INTEGER PRIMARY KEY,
  downloadPercent INTEGER
);

insertOrReplace:
INSERT OR REPLACE
INTO PodcastDownload
VALUES ?;

selectAll:
SELECT *
FROM PodcastDownload;

selectByEntryId:
SELECT *
FROM PodcastDownload
WHERE entryId = ?;

deleteByEntryId:
DELETE
FROM PodcastDownload
WHERE entryId = ?;
CREATE TABLE Feed (
  id INTEGER PRIMARY KEY,
  url TEXT NOT NULL,
  title TEXT NOT NULL,
  faviconLink TEXT NOT NULL,
  link TEXT NOT NULL,
  updateErrorCount INTEGER NOT NULL,
  lastUpdateError TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO Feed
VALUES ?;

selectAll:
SELECT *
FROM Feed
ORDER BY title;

selectById:
SELECT *
FROM Feed
WHERE id = ?;

selectCount:
SELECT COUNT(*)
FROM Feed;

deleteAll:
DELETE
FROM Feed;

deleteById:
DELETE
FROM Feed
WHERE id = ?;
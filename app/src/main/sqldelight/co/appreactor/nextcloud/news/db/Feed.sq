CREATE TABLE Feed (
  id INTEGER PRIMARY KEY,
  url TEXT NOT NULL,
  title TEXT NOT NULL,
  faviconLink TEXT NOT NULL,
  link TEXT NOT NULL,
  updateErrorCount INTEGER NOT NULL,
  lastUpdateError TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO Feed
VALUES ?;

findAll:
SELECT *
FROM Feed
ORDER BY title;

findById:
SELECT *
FROM Feed
WHERE id = :id;

count:
SELECT COUNT(*)
FROM Feed;

deleteAll:
DELETE
FROM Feed;

deleteById:
DELETE
FROM Feed
WHERE id = :id;
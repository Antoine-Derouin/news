CREATE TABLE EntryEnclosure (
  entryId TEXT PRIMARY KEY NOT NULL,
  downloadPercent INTEGER
);

insertOrReplace:
INSERT OR REPLACE
INTO EntryEnclosure
VALUES ?;

selectAll:
SELECT *
FROM EntryEnclosure;

selectByEntryId:
SELECT *
FROM EntryEnclosure
WHERE entryId = ?;

deleteByEntryId:
DELETE
FROM EntryEnclosure
WHERE entryId = ?;
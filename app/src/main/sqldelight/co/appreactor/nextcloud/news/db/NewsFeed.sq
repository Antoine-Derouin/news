CREATE TABLE NewsFeed (
  id INTEGER PRIMARY KEY,
  title TEXT NOT NULL,
  faviconLink TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO NewsFeed
VALUES ?;

findAll:
SELECT *
FROM NewsFeed;

findById:
SELECT *
FROM NewsFeed
WHERE id = :id;

count:
SELECT COUNT(*)
FROM NewsFeed;

deleteAll:
DELETE
FROM NewsFeed;
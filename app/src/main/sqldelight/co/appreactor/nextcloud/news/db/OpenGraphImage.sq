CREATE TABLE OpenGraphImage (
  entryId TEXT PRIMARY KEY NOT NULL,
  status TEXT NOT NULL,
  url TEXT NOT NULL,
  width INTEGER NOT NULL,
  height INTEGER NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO OpenGraphImage
VALUES ?;

selectAll:
SELECT *
FROM OpenGraphImage;

selectByEntryId:
SELECT *
FROM OpenGraphImage
WHERE entryId = ?;
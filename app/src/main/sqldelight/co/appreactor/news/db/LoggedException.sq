CREATE TABLE LoggedException (
  id TEXT PRIMARY KEY,
  date TEXT NOT NULL,
  exceptionClass TEXT NOT NULL,
  message TEXT NOT NULL,
  stackTrace TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO LoggedException
VALUES ?;

selectAll:
SELECT *
FROM LoggedException
ORDER BY date DESC;

selectById:
SELECT *
FROM LoggedException
WHERE id = ?;

selectCount:
SELECT COUNT(*)
FROM LoggedException;

deleteAll:
DELETE
FROM LoggedException;

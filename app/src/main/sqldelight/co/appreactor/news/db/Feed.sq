CREATE TABLE Feed (
  id TEXT PRIMARY KEY NOT NULL,
  title TEXT NOT NULL,
  link TEXT NOT NULL,
  alternateLink TEXT NOT NULL,
  alternateLinkType TEXT NOT NULL
);

insertOrReplace:
INSERT OR REPLACE
INTO Feed
VALUES ?;

selectAll:
SELECT *
FROM Feed
ORDER BY title;

selectById:
SELECT *
FROM Feed
WHERE id = ?;

selectCount:
SELECT COUNT(*)
FROM Feed;

deleteAll:
DELETE
FROM Feed;

deleteById:
DELETE
FROM Feed
WHERE id = ?;